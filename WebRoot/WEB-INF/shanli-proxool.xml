<?xml version="1.0" encoding="UTF-8"?>
<slrad>
	<!-- proxool数据源配置 ,支持多个数据源 -->
	<proxool>
		<alias>dblc</alias>
		<driver-url>jdbc:mysql://127.0.0.1:3306/db_lc</driver-url>
		<driver-class>com.mysql.jdbc.Driver</driver-class>
		<driver-properties>
			<property name="useUnicode" value="true"/>
			<property name="characterEncoding" value="UTF-8"/>
			<property name="user" value="root" />
			<property name="password" value="5bbe429c61" />
		</driver-properties>
		<maximum-connection-count>100</maximum-connection-count>
		<minimum-connection-count>5</minimum-connection-count>
		<house-keeping-test-sql>select 0</house-keeping-test-sql>
			<trace>true</trace>
		<maximum-active-time>1800000</maximum-active-time>
	</proxool> 
